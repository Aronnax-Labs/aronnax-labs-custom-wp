{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aronnax/navbar",
  "version": "1.2.0",
  "title": "Navbar",
  "category": "theme",
  "icon": "menu",
  "description": "Responsive navigation bar",
  "supports": {
    "html": false
  },
  "attributes": {
    "logoImage": {
      "type": "string",
      "default": ""
    },
    "siteTitle": {
      "type": "string",
      "default": "Site Title"
    },
    "homeUrl": {
      "type": "string",
      "default": "/"
    },
    "menuItems": {
      "type": "array",
      "default": []
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "logoColor": {
      "type": "string",
      "default": "#333333"
    },
    "menuColor": {
      "type": "string",
      "default": "#666666"
    },
    "borderWidth": {
      "type": "number",
      "default": 1
    },
    "borderColor": {
      "type": "string",
      "default": "#e0e0e0"
    },
    "hamburgerColor": {
      "type": "string",
      "default": "#333333"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
