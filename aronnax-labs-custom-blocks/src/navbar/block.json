{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "aronnax/navbar",
  "title": "Navbar",
  "category": "theme",
  "icon": "menu",
  "description": "Responsive navigation bar",
  "supports": {
    "html": false
  },
  "attributes": {
    "logoImage": {
      "type": "string",
      "default": ""
    },
    "siteTitle": {
      "type": "string",
      "default": "Site Title"
    },
    "homeUrl": {
      "type": "string",
      "default": "/"
    },
    "menuItems": {
      "type": "array",
      "default": [
        {"label": "Home", "url": "#", "newTab": false},
        {"label": "About", "url": "#", "newTab": false},
        {"label": "Services", "url": "#", "newTab": false},
        {"label": "Contact", "url": "#", "newTab": false}
      ]
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "logoColor": {
      "type": "string",
      "default": "#333333"
    },
    "menuColor": {
      "type": "string",
      "default": "#666666"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
